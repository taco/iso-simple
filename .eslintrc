{
    "ecmaFeatures": {
    	"arrowFunctions": true,
    	"binaryLiterals": true,
    	"blockBindings": true,
    	"classes": true,
    	"defaultParams": true,
    	"destructuring": true,
        "generators": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
    	"octalLiterals": true,
    	"spread": true,
    	"superInFunctions": true,
    	"templateStrings": true,
    	"modules": true,
        "forOf": true,
        "jsx": true
    }, {
    	"env": {
    		"node": true
    	}
    },
    "rules": {
        "comma-dangle": 2,
        "eqeqeq": 2,
        "quotes": [2, 'single'],
        "semi": [2, 'never'],
        "no-extra-semi": 2,
        "arrow-spacing": [2, { "before": true, "after": true}]
    }
}